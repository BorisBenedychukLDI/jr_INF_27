<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity09lio">

    <ImageView
        android:id="@+id/bg_09lio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg_jr_09lio" />

    <Button
        android:id="@+id/b_09lio"
        android:layout_width="164dp"
        android:layout_height="59dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/button_bg_09lio"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gl_09lio_main" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_09lio_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.500684" />

    <ImageView
        android:id="@+id/iv_logo_09lio"
        android:layout_width="206dp"
        android:layout_height="142dp"
        android:layout_marginBottom="30dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_jr_09lio"
        app:layout_constraintBottom_toTopOf="@+id/gl_09lio_main"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/pb_09lio"
        style="?android:attr/progressBarStyle"
        android:layout_width="40dp"
        android:layout_height="39dp"
        android:indeterminateTint="@color/theme_2"
        app:layout_constraintBottom_toTopOf="@+id/gl_09lio_main"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gl_09lio_main" />


    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottie_main_09lio"
        android:layout_width="136dp"
        android:layout_height="48dp"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/b_09lio"
        app:layout_constraintEnd_toEndOf="@+id/b_09lio"
        app:layout_constraintStart_toStartOf="@+id/b_09lio"
        app:layout_constraintTop_toTopOf="@+id/b_09lio"
        app:lottie_autoPlay="false"
        app:lottie_loop="false"
        app:lottie_rawRes="@raw/start_09lio" />

</androidx.constraintlayout.widget.ConstraintLayout>